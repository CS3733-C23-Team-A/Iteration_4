<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.wpi.cs3733.C23.teamA.hibernateDB.LocationNameEntity" table="locationname" schema="public"
           catalog="dba">
        <id name="longname">
            <column name="longname" sql-type="text"/>
        </id>
        <property name="shortname">
            <column name="shortname" sql-type="text" not-null="true"/>
        </property>
        <property name="locationtype">
            <column name="locationtype" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>