<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.wpi.cs3733.C23.teamA.Database.Entities.ComputerRequestEntity" table="computerrequest" schema="employeetester"
           catalog="dba">
        <id name="requestid">
            <column name="requestid" sql-type="integer"/>
        </id>
        <property name="deviceid">
            <column name="deviceid" sql-type="text" not-null="true"/>
        </property>
        <property name="device">
            <column name="device" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>